#include <iostream>
using namespace std;

int main() {
    int Score[5];

    Score[0] = 10;
    Score[1] = 12;
    Score[2] = 18;
    Score[3] = 101;
    Score[4] = 23;

    cout << "Score 1: " << Score[0] << endl;
    cout << "Score 2: " << Score[1] << endl;
    cout << "Score 3: " << Score[2] << endl;
    cout << "Score 4: " << Score[3] << endl;
    cout << "Score 5: " << Score[4] << endl;

    return 0;
}
